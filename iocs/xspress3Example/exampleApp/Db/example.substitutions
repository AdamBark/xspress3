file NDFile.template {
	pattern {P, R, PORT, TIMEOUT, ADDR}
		{mp49:, XSP3:HDF5:, XSP3.HDF5, 1, 0}
}


file NDFileHDF5.template {
	pattern {P, R, PORT}
		{mp49:, XSP3:HDF5:, XSP3.HDF5}
}

file xspress3_highlevel.template {
	pattern {P, R, HDF, PROC}
		{mp49:, XSP3:, mp49:XSP3:HDF5:, mp49:XSP3:PROC:}
}

file NDProcess.template
{
	pattern {P, R, PORT, ADDR, TIMEOUT}
		{mp49:,XSP3:PROC:, XSP3.PROC, 0, 1}
}

