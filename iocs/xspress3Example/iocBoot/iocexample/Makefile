TOP = ../..
include $(TOP)/configure/CONFIG

SCRIPTS+=stexample.boot
SCRIPTS+=xspress3-ioc.sh

USR_BOOT_FLAGS+=-MXSPRESS3_PREFIX=$(XSPRESS3_PREFIX)
USR_BOOT_FLAGS+=-MXSPRESS3_CONFIG=$(XSPRESS3_CONFIG)
USR_BOOT_FLAGS+=-MXSPRESS3_SAVE=$(XSPRESS3_SAVE)
USR_BOOT_FLAGS+=-MXSPRESS3_DATA=$(XSPRESS3_DATA)
USR_BOOT_FLAGS+=-MXSPRESS3_AUTOSAVE=$(XSPRESS3_AUTOSAVE)
USR_BOOT_FLAGS+=-MXSPRESS3_NUM_CHANNELS=$(XSPRESS3_NUM_CHANNELS)

include $(TOP)/configure/RULES

xspress3-ioc.sh: ../xspress3-ioc.sh
	$(MSI) $(SYS_BOOT_FLAGS) $< > $@
